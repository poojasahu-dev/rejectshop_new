			<property qualifier="AbstractBannerComponent.urlLink"/>
			<property qualifier="ProductBannerCMSComponent.price"/>
			<property qualifier="ProductBannerCMSComponent.unit"/>
			<property qualifier="ProductBannerCMSComponent.amountSaved"/>
		</group>
	</displayed-properties>
</wizard-config>
